[Unit]
Description=SSHHD service for fast SSH
After=network-online.target
 
[Service]
Type=simple

User=root
Group=root


ExecStartPre=echo "" >> 
ExecStart=bash -c "sed -i '$a coucou' 12;"

ExecStartPre=echo "" >> 
ExecStart=bash -c "sed -i '$a coucou' 13;"


ExecStop=bash -c " sed -i '$a coucou' 12 &&  sed -i '$a coucou' 13 && ls;"
[Install]
WantedBy=multi-user.target
